#!/usr/bin/env bash

cd "$HOME"
xhost localhost
xhost si:localuser:root
xrdb -merge .Xresources
xbindkeys
update-xinput
xscreensaver -no-splash &
xcompmgr &
start-pulseaudio-x11
parcellite &
blueman-applet &
/usr/lib/x86_64-linux-gnu/libexec/polkit-kde-authentication-agent-1 &
set-background
cd Desktop
i3
