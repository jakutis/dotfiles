#!/usr/bin/env bash

xhost localhost
xrdb -merge "$HOME/.Xresources"
xbindkeys
update-xinput
su -c "echo 0 > /sys/devices/platform/thinkpad_acpi/rfkill/rfkill0/state"
sudo wpa_supplicant -c/etc/wpa_supplicant.conf -iwlp3s0 -B
sudo ifconfig wlp3s0 down
sudo ifconfig enp0s25 down
google-drive-ocamlfuse -skiptrash -headless -m /mnt/google
klipper
system screens mobile
xscreensaver -no-splash &
awesome
