#!/bin/bash

wget --user-agent="Mozilla/5.0 (X11; Linux x86_64) AppleWebKit/537.36 (KHTML, like Gecko) Chrome/36.0.1985.143 Safari/537.36" -e robots=off --no-parent --force-directories --recursive --timestamping --level=inf --convert-links --backup-converted --no-remove-listing --page-requisites "$@"
