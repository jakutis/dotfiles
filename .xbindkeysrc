# To find out a key sequence definition, run xbindkeys -mk

# Set screen to first only
"system screens only FIRST"
  Mod4 + F3

# Reset screens to only the first connected
"system screens"
  Mod4 + F2

# Keyboard brightness raise
"system keyboard more"
  XF86KbdBrightnessUp

# Keyboard brightness lower
"system keyboard less"
  XF86KbdBrightnessDown

# Screen brightness raise
"system brightness more"
  XF86MonBrightnessUp

# Screen brightness lower
"system brightness less"
  XF86MonBrightnessDown

# Win+F8
"xclip -o -selection primary|xclip -i -selection clipboard"
  Mod4 + F8

# Win+PageUp
"volume pulse increase"
  Mod4 + Prior

# Audio Raise
"volume pulse increase"
  XF86AudioRaiseVolume

# Win+PageDown
"volume pulse decrease"
  Mod4 + Next

# Audio Lower
"volume pulse decrease"
  XF86AudioLowerVolume

# Win+Power
"system suspend"
  Mod4 + XF86PowerOff

# Win+End
"system suspend"
  Mod4 + End

# Win+Home
"displayoff;xscreensaver-command -lock"
  Mod4 + Home

# Print Screen
"screenshot"
  Print

# Print Screen
"screenshot"
  XF86LaunchA

# Win+Z
"cmus-remote -C player-prev"
  Mod4 + z

# Win+X
"cmus-remote -C player-play"
  Mod4 + x

# Win+C
"cmus-remote -C player-pause"
  Mod4 + c

# Win+V
"cmus-remote -C player-stop"
  Mod4 + v

# Win+B
"cmus-remote -C player-next"
  Mod4 + b
