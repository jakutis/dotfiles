# To find out a key sequence definition, run xbindkeys -mk

# XF86MonBrightnessDown
"system brightness less"
  m:0x0 + c:232

# XF86MonBrightnessUp
"system brightness more"
  m:0x0 + c:233

# XF86WebCam
"blank"
  m:0x0 + c:220

# XF86Display
"switch-display"
  m:0x0 + c:235

# XF86WLAN
"system net next"
  m:0x0 + c:246

# Win+C
"xclip -o -selection primary|xclip -i -selection clipboard"
  m:0x40 + c:54

# Win+PageUp
"volume pulse increase"
  m:0x40 + c:112

# Win+PageDown
"volume pulse decrease"
  m:0x40 + c:117

# Win+S
"xkbswitch"
  m:0x50 + c:39

# Print Screen
"ksnapshot --current"
  m:0x0 + c:107
