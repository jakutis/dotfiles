# To find out a key sequence definition, run xbindkeys -mk

# Win+F8
"xclip -o -selection primary|xclip -i -selection clipboard"
  Mod4 + F8

# Win+PageUp
"volume pulse increase"
  Mod4 + Prior

# Audio Raise
"volume pulse increase"
  XF86AudioRaiseVolume

# Win+PageDown
"volume pulse decrease"
  Mod4 + Next

# Audio Lower
"volume pulse decrease"
  XF86AudioLowerVolume

# Win+S
"xkbswitch"
  Mod4 + s

# Win+Home
"blank;xscreensaver-command -lock"
  Mod4 + Home

# Print Screen
"shutter -f"
  Print

# Win+Z
"cmus-remote -C player-prev"
  Mod4 + z

# Win+X
"cmus-remote -C player-play"
  Mod4 + x

# Win+C
"cmus-remote -C player-pause"
  Mod4 + c

# Win+V
"cmus-remote -C player-stop"
  Mod4 + v

# Win+B
"cmus-remote -C player-next"
  Mod4 + b
