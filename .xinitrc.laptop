#!/usr/bin/env bash

cd "$HOME"
source .profile
xhost localhost
xrdb -merge .Xresources
update-xinput
LOG_DIR="$HOME/etc/wiki/xwinlogger" nohup xwinlogger >/dev/null 2>&1 &
start-pulseaudio-x11
start-minidlna
xscreensaver -no-splash &
autocutsel -selection PRIMARY -buttonup -fork
autocutsel -selection SECONDARY -buttonup -fork
autocutsel -selection CLIPBOARD -buttonup -fork
awesome
