#!/usr/bin/env bash

xhost localhost
xrdb -merge "$HOME/.Xresources"
xbindkeys
update-xinput
xscreensaver -no-splash &
xcompmgr &
start-pulseaudio-x11
parcellite &
blueman-applet &
nohup ./.dropbox-dist/dropboxd 1>/dev/null 2>/dev/null &
nohup redshift -b 0.3:0.3 -t 2700K:2700K 1>/dev/null 2>&1 &
cd Desktop
awesome
